#: src/hello.c:228
#, c-format
msgid "%s: Incompatible flags: -m and -t\n"
msgstr "%s：标记 -m 和 -t 矛盾\n"

#: src/hello.c:260
#, c-format
msgid "%s: Who are you?\n"
msgstr "%s：你是谁？\n"

#: src/hello.c:282
#, c-format
msgid "%s: Cannot find your mail spool file.\n"
msgstr "%s：找不到您的邮件卷轴文件。\n"

#: src/hello.c:326
msgid "Nothing happens here."
msgstr "这儿什么也没发生。"

#: src/hello.c:332
msgid "hello, world\n"
msgstr "世界你好\n"

