#, c-format
msgid ""
"+---------------+\n"
"| Hello, world! |\n"
"+---------------+\n"
msgstr ""
"+----------------+\n"
"| Поздрав свима! |\n"
"+----------------+\n"

#: src/hello.c:344
msgid "Hello, world!"
msgstr "Здраво, свете!"

#: src/hello.c:359
#, c-format
msgid "%s: Virtual memory exhausted\n"
msgstr "%s: Виртуална меморија исцрпљена\n"
