msgid ""
"+---------------+\n"
"| Hello, world! |\n"
"+---------------+\n"
msgstr ""
"┏━━━━━━━━━┓\n"
"┃  Chào thế giới!     ┃\n"
"┗━━━━━━━━━┛\n"

#: src/hello.c:344
msgid "Hello, world!"
msgstr "Chào thế giới!"

#: src/hello.c:359
#, c-format
msgid "%s: Virtual memory exhausted\n"
msgstr "%s: Hết bộ nhớ ảo\n"
