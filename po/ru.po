#: src/getopt.c:693
#, c-format
msgid "%s: option `%s' is ambiguous\n"
msgstr "%s: неоднозначный ключ `%s'\n"

#: src/getopt.c:718
#, c-format
msgid "%s: option `--%s' doesn't allow an argument\n"
msgstr "%s: ключ `--%s' должен использоваться без аргумента\n"

#: src/getopt.c:723
#, c-format
msgid "%s: option `%c%s' doesn't allow an argument\n"
msgstr "%s: ключ `%c%s' должен использоваться без аргумента\n"

#: src/getopt.c:741 src/getopt.c:914
#, c-format
msgid "%s: option `%s' requires an argument\n"
msgstr "%s: ключ `%s' должен использоваться c аргументом\n"

#: src/getopt.c:770
#, c-format
msgid "%s: unrecognized option `--%s'\n"
msgstr "%s: неизвестный ключ `--%s'\n"

#: src/getopt.c:774
#, c-format
msgid "%s: unrecognized option `%c%s'\n"
msgstr "%s: неизвестный ключ `%c%s'\n"

#: src/getopt.c:800
#, c-format
msgid "%s: illegal option -- %c\n"
msgstr "%s: недопустимый ключ -- %c\n"

#: src/getopt.c:803
#, c-format
msgid "%s: invalid option -- %c\n"
msgstr "%s: неверный ключ -- %c\n"

#: src/getopt.c:833 src/getopt.c:963
#, c-format
msgid "%s: option requires an argument -- %c\n"
msgstr "%s: ключ %c должен бы использоваться c аргументом\n"

#: src/getopt.c:880
#, c-format
msgid "%s: option `-W %s' is ambiguous\n"
msgstr "%s: неоднозначный ключ `-W %s'\n"

#: src/getopt.c:898
#, c-format
msgid "%s: option `-W %s' doesn't allow an argument\n"
msgstr "%s: ключ `-W %s' должен использоваться без аргумента\n"

#: src/hello.c:152
msgid "Too many arguments\n"
msgstr "Слишком много аргументов\n"

#: src/hello.c:153
#, c-format
msgid "Try `%s --help' for more information.\n"
msgstr "Попробуйте `%s --help' для получения более подробного описания.\n"

#. TRANSLATORS: --help output 1
#. no-wrap
#: src/hello.c:165
msgid "GNU hello, THE greeting printing program.\n"
msgstr "GNU hello, та самая программа, печатающая приветствие.\n"

#. TRANSLATORS: --help output 2
#. no-wrap
#: src/hello.c:170
#, c-format
msgid "Usage: %s [OPTION]\n"
msgstr "Использование: %s [КЛЮЧ]\n"

#. TRANSLATORS: --help output 3 : options 1/2
#. no-wrap
#: src/hello.c:176
msgid ""
"  -h, --help          display this help and exit\n"
"  -v, --version       display version information and exit\n"
msgstr ""
"  -h, --help          показать эту справку и выйти\n"
"  -v, --version       показать информацию о версии и выйти\n"

#. TRANSLATORS: --help output 4 : options 2/2
#. no-wrap
#: src/hello.c:183
#, fuzzy
msgid ""
"  -t, --traditional       use traditional greeting format\n"
"  -n, --next-generation   use next-generation greeting format\n"
"  -g, --greeting=TEXT     use TEXT as the greeting message\n"
"  -m, --mail              print your mail\n"
msgstr ""
"  -t, --traditional       использовать традиционный вид приветствия\n"
"  -n, --next-generation   использовать формат приветствия нового поколения\n"
"  -m, --mail              напечатать вашу почту\n"

#. TRANSLATORS: --help output 5 (end)
